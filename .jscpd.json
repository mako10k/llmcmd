{
  "threshold": 0,
  "minTokens": 50,
  "reporters": ["console", "json", "html"],
  "output": "jscpd-report",
  "include": [
    "**/*.go",
    "llmsh-rs/src/**/*.rs"
  ],
  "exclude": [
  "**/vendor/**",
  "**/node_modules/**",
    ".git/**",
    ".github/**",
    "docs/**",
    "logs/**",
    "release/**",
    "releases/**",
    "jscpd-report/**",
    "coverage/**",
    "target/**",
    "llmsh-rs/target/**",
    "bin/**",
    "data/**",
    "test_data/**",
    "**/*.min.*"
  ]
}
